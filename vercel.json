{
  "rewrites": [
    {
      "source": "/tinymce/js/tinymce/tinymce.min.js",
      "destination": "/public/tinymce/js/tinymce/tinymce.min.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/tinymce/js/tinymce/tinymce.min.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
